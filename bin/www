#!/usr/bin/env node

/**
 * Module dependencies.
 */

var app = require('../app');
var debug = require('debug')('application:server');
var http = require('http');

/**
 * Get port from environment and store in Express.
 */

 let port = process.env.PORT;
 if (port == null || port == "") {
   port = 3000;
 }
 app.listen(port, function() {
   console.log("Server started on port 3000");
 });
